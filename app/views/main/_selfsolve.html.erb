      <% end %>
    </div>  
  </div>
</div>

<div class="panel">
  <input type="button" class="btn btn-primary btn-block" onclick="set_selfsolve_results();" value="Завершить решение">
  <input type="button" class="btn btn-danger btn-block " id="btn-victim<%= i %><%= j %>" onclick="update_selfsolve();" value="Начать решение сначала">
</div>

